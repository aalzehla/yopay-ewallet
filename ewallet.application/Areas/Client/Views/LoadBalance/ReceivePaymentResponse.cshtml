@model ewallet.shared.Models.LoadBalance.ViewTransactionReponseModel

@{
    ViewBag.Title = "ReceivePaymentResponse";
    Layout = "~/Areas/Client/Views/Shared/_Layout.cshtml";
}

<div class="card">
    <div class="card-body">
        <fieldset class="mb-1">
            <div class="row">
                <div class="col-md-6">
                    @Html.NLabelFor(model => model.pmt_gateway_txn_id, new { @class = "form-control", autocomplete = "off" }, 2)
                    @Html.NLabelFor(model => model.pmt_txn_id, new { @class = "form-control", autocomplete = "off" }, 2)
                    @Html.NLabelFor(model => model.amount, new { @class = "form-control", autocomplete = "off" }, 2)
                    @Html.NLabelFor(model => model.agent_id, new { @class = "form-control", autocomplete = "off" }, 2)
                    @Html.NLabelFor(model => model.user_name, new { @class = "form-control", autocomplete = "off" }, 2)
                </div>
            </div>
        </fieldset>
    </div>
</div>

